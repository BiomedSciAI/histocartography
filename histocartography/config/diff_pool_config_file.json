{
  "graph_building": {
    "graph_building_type": "knn_graph_builder",
    "max_num_nodes": 5000,
    "edge_encoding": false,
    "edge_threshold": 0.5
  },
  "gnn": {
    "layer_type": "gnn",
    "num_layers": 5,
    "node_dim": 2,
    "activation": "relu",
    "neighbor_pooling_type": "sum",
    "pooling": {
      "pooling_type": "all",
      "readout": {
        "num_layers": 2,
        "hidden_dim": 64,
        "out_dim": 15
      }
    }
  },
  "model_type": "DiffPool",
  "model_params": {
    "num_classes": 2,
    "dropout": 0,
    "use_bn": false,
    "pool_ratio": 0.3,
    "cat": false,
    "num_aggs": 1,
    "hidden_dim": 10,
    "output_dim": 10,
    "gnn_params": [
      {
        "layer_type": "gin_layer",
        "activation": "relu",
        "n_layers": 2,
        "neighbor_pooling_type": "mean"
      },
      {
        "layer_type": "dense_gin_layer",
        "activation": "relu",
        "n_layers": 2,
        "neighbor_pooling_type": "mean"
      }
    ],
    "pooling_params": [
      {
        "layer_type": "diff_pool_layer",
        "activation": "relu",
        "neighbor_pooling_type": "mean"
      }
    ]
  }
}